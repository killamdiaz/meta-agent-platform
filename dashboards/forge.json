{
  "title": "Forge Overview",
  "panels": [
    { "type": "graph", "title": "API latency", "targets": [{ "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))" }] },
    { "type": "graph", "title": "Token usage", "targets": [{ "expr": "tokens_consumed_total" }] },
    { "type": "graph", "title": "Errors per minute", "targets": [{ "expr": "rate(error_count_total[1m])" }] },
    { "type": "graph", "title": "License violations", "targets": [{ "expr": "license_validation_failures_total" }] }
  ]
}
